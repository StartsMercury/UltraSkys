{
  "schema_version": 1,

  "quilt_loader": {
    "group": "${group}",
    "id": "${modid}",
    "version": "${version}",

    "intermediate_mappings":"net.fabricmc:intermediary",

    "metadata": {
      "name": "HgSkys",
      "description": "HydrargyrumSkys (HgSkys) is a fork of UltraSkys",
      "contributors": {
        "Nano": "Owner of UltraSkys",
        "StartsMercury": "Owner"
      },

      "contact": {
        "homepage": "https://github.com/StartsMercury/UltraSkys/tree/builds",
        "issues": "https://github.com/StartsMercury/UltraSkys/issues",
        "sources": "https://github.com/StartsMercury/UltraSkys/tree/builds"
      }
    },

    "entrypoints": {
      "init": "com.github.ultraskys.UltraSkys"
    },

    "depends": [
      {
        "id": "cosmic_quilt",
        "versions": "*"
      },
      {
        "id": "cosmic_reach",
        "versions": "*"
      }
    ],

    "provides": [ "ultraskys" ]
  },

  "mixin": "ultraskys.mixins.json"
}
